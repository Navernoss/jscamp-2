{
  "name": "javascriptcamp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "upgrade": "yarn upgrade @docusaurus/core@2.0.0-beta.8 @docusaurus/preset-classic@2.0.0-beta.8",
    "gitpush": "GIT_USER=gHashTag CURRENT_BRANCH=gh-pages USE_SSH=true yarn deploy",
    "push": "yarn build && gh-pages -b master -d build",
    "ci": "yarn lint && yarn prettier:diff",
    "lint": "eslint --cache \"**/*.js\" && stylelint \"**/*.css\"",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,md}\"",
    "prettier:diff": "prettier --config .prettierrc --list-different \"**/*.{js,md}\"",
    "ru": "yarn run start -- --locale ru",
    "en": "yarn run start -- --locale en",
    "zh": "yarn run start -- --locale zh",
    "hi": "yarn run start -- --locale hi",
    "ar": "yarn run start -- --locale ar"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.8",
    "@docusaurus/preset-classic": "2.0.0-beta.8",
    "@docusaurus/remark-plugin-npm2yarn": "^2.0.0-beta.8",
    "@docusaurus/theme-live-codeblock": "2.0.0-beta.8",
    "@docusaurus/theme-search-algolia": "^2.0.0-beta.8",
    "caniuse-lite": "^1.0.30001204",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "gh-pages": "^3.1.0",
    "prism-react-renderer": "^1.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-youtube": "^7.11.2",
    "remark-snackplayer": "^0.0.3"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.19.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-airbnb": "^5.0.0",
    "babel-preset-react-native": "^4.0.1",
    "docusaurus-plugin-google-adsense": "^0.1.0",
    "eslint": "^7.23.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-header": "^3.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.10.0",
    "eslint-watch": "^7.0.0",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "stylelint": "^13.2.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
